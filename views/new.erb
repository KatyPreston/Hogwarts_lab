<form method="post" action="/students">
<label for="first-name"> First Name:</label>
<input type="text" name="first_name" id="first-name"  placeholder="put your name here">
<label for="last-name"> Last Name:</label>
<input type="text" name="last_name" id="last-name"  placeholder="put your last name here">

<label for="House-select"> Select a House:</label>
<select name="house" id="house-select">
<option value="Gryffindor"> Gryffindor</option>
<option value="RavenClaw"> RavenClaw </option>
<option value="Slytherin"> Slytherin </option>
<option value="Hufflepuff"> Hufflepuff </option>
</select>

<label for="age">Age: </label>
<input type="number" name="age" id="age">

  <input type="submit" value="Register new student">
</form>
